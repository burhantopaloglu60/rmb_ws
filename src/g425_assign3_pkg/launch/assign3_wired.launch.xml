<launch>
  <!-- Micro-ROS Agent -->
  <node pkg="g425_assign3_pkg" exec="ESP32Subscriber" >
  </node>
  <node pkg="g425_assign3_pkg" exec="ImuLifecycleNode" >
  </node>
  <node pkg="g425_assign3_pkg" exec="LifecycleManager" >
  </node>
  <node
    pkg="micro_ros_agent"
    exec="micro_ros_agent"
    args="serial --dev /dev/ttyUSB0 -b 115200"
    output="screen">
  </node>


