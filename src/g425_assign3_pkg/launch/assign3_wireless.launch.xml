    <param from="$(find-pkg-share g425_assign3_pkg)/config/assign3_params.yaml" />
    <param name="connection_type" value="true"/>
    </node>
    <node pkg="g425_assign3_pkg" exec="ImuLifecycleNode" >
    <param from="$(find-pkg-share g425_assign3_pkg)/config/assign3_params.yaml" />
    <param name="connection_type" value="true"/>
    </node>
    <node pkg="g425_assign3_pkg" exec="LifecycleManager" >
    <param from="$(find-pkg-share g425_assign3_pkg)/config/assign3_params.yaml" />
    <param name="connection_type" value="true"/>
    </node>
</launch>